&ts_int_active {
	config {
		/delete-property/ bias-pull-down;
		bias-pull-up;
	};
};

&pmi632_gpios {
	submainboard {
		sub_mainboard_default: sub_mainboard_default{
			pins = "gpio3";
			function = "normal"; /* normal */
			bias-high-impedance; /* disable GPIO for ADC*/
			bias-disable;
		};
	};
	gpio1_adc {
		gpio1_adc_default: gpio1_adc_default {
			pins = "gpio1"; /* GPIO 1 */
			function = "normal"; /* normal */
			bias-high-impedance; /* DISABLE GPIO1 for ADC*/
		};
	};

	gpio4_adc {
		gpio4_adc_default: gpio4_adc_default {
			pins = "gpio4"; /* GPIO 4 */
			function = "normal"; /* normal */
			bias-high-impedance; /* DISABLE GPIO4 for ADC*/
			};
	};

	charger {
		dischg_enable: dischg_enable{
			pins = "gpio2";
			function = "normal";
			qcom,drive-strength = <2>;
			power-source = <0>;
			bias-disable;
			output-high;
		};

		dischg_disable: dischg_disable{
			pins = "gpio2";
			function = "normal";
			qcom,drive-strength = <2>;
			power-source = <0>;
			bias-disable;
			output-low;
		};
	};
};
