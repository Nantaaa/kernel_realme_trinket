	charger: charger {
		usbid_active: usbid_active {
			mux {
				pins = "gpio47";
				function = "gpio";
			};

			config {
				pins = "gpio47";
				drive-strength = <2>;
				input-enable;
				bias-pull-up;
			};
		};

		usbid_sleep: usbid_sleep {
