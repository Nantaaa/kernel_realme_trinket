&qupv3_se2_spi {
	pinctrl-0 = <&qupv3_se2_spi_active &qupv3_se2_spi_cs0_active &qupv3_se2_spi_miso_active>;
	pinctrl-1 = <&qupv3_se2_spi_sleep &qupv3_se2_spi_cs0_sleep &qupv3_se2_spi_miso_sleep>;
};

&qupv3_se2_spi_active {
	mux {
		pins = "gpio7", "gpio8";
	};

	config {
		pins = "gpio7", "gpio8";
		output-low;
	};
};

&qupv3_se4_2uart_sleep {
	input-enable;
};

&qupv3_se2_spi_sleep {
	mux {
		pins = "gpio7", "gpio8";
		function = "gpio";
	};

	configs {
		pins = "gpio7", "gpio8";
		drive-strength = <6>;
		output-low;
		bias-disable;
	};
};

&tlmm {
	gpio_id1_default: gpio_id1_default {
		mux {
			pins = "gpio113";
			function = "gpio";
		};

		config {
			pins = "gpio113";
			drive-strength = <2>;
			bias-pull-up;
			input-enable;
		};
	};

	qupv3_se4_2uart_pins: qupv3_se4_2uart_pins {
		qupv3_se4_2uart_active: qupv3_se4_2uart_active {
			config {
				input-enable;
			};
		};
	};

	qupv3_se2_spi_pins: qupv3_se2_spi_pins {
		qupv3_se2_spi_miso_active: qupv3_se2_spi_miso_active {
			mux {
				pins = "gpio6";
				function = "qup02";
			};

			config {
				pins = "gpio6";
				drive-strength = <6>;
				bias-disable;
			};
		};

		qupv3_se2_spi_miso_sleep: qupv3_se2_spi_miso_sleep {
			mux {
				pins = "gpio6";
				function = "gpio";
			};

			configs {
				pins = "gpio6";
				drive-strength = <6>;
				bias-disable;
			};
		};
		qupv3_se2_spi_cs0_active: qupv3_se2_spi_cs0_active {
			mux {
				pins = "gpio9";
				function = "qup02";
			};

			config {
				pins = "gpio9";
				drive-strength = <6>;
				output-high;
				bias-disable;
			};
		};

		qupv3_se2_spi_cs0_sleep: qupv3_se2_spi_cs0_sleep {
			mux {
				pins = "gpio9";
				function = "gpio";
			};

			configs {
				pins = "gpio9";
				drive-strength = <6>;
				output-high;
				bias-disable;
			};
		};
	};

	cam_sensor_front_standby_active: cam_sensor_front_standby_active {
		/* RESET  */
		mux {
			pins = "gpio119";
			function = "gpio";
		};

		config {
			pins = "gpio119";
			bias-disable; /* No PULL */
			drive-strength = <2>; /* 2 MA */
		};
	};

	cam_sensor_front_standby_suspend: cam_sensor_front_standby_suspend {
		/* RESET */
		mux {
